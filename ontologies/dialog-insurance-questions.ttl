# Q1: First Name
:FirstNameQuestion a mm:MultimodalQuestion ;
    rdfs:label "First Name" ;
    :questionId "q_first_name" ;
    :questionText "What is your first name?" ;
    :slotName "firstName" ;
    :required true ;
    :spellingRequired true ;
    :order 10 ;
    :inSection :PersonalInfoSection ;
    :hasInputMode vocab:NameInputMode ;
    :hasForm :PersonalDetailsForm ;
    mm:hasTTSPrompt :FirstNameTTS ;
    mm:fieldType "text" ;
    mm:placeholder "Enter your first name" ;
    mm:helpText "Please provide your legal first name as it appears on your ID" ;
    mm:minLength 2 ;
    mm:maxLength 50 ;
    mm:validationRule "isAlpha" ;
    mm:validationMessage "First name must contain only letters" ;
    mm:documentFillable true ;
    mm:uiHint "half-width" .

:FirstNameTTS a mm:TTSPrompt ;
    mm:ttsText "Please tell me your first name." ;
    mm:ttsVariant1 "Please tell me your first name." ;
    mm:ttsVariant2 "Could you provide your first name, please?" ;
    mm:ttsVariant3 "What's your first name?" ;
    mm:ttsVariant4 "I need your first name." ;
    mm:ttsVoice "en-GB-Neural2-A" ;
    mm:ttsRate 1.0 ;
    mm:ttsPitch 1.0 .


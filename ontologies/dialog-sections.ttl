:Driver2YearsHoldingLicenceQuestion :inSection :AdditionalDriversSection .
:Driver2ConvictionsQuestion :inSection :AdditionalDriversSection .
:Driver2ClaimsQuestion :inSection :AdditionalDriversSection .
:Driver2OccupationQuestion :inSection :AdditionalDriversSection .

# Occupation Details Section Questions
:OccupationQuestion :inSection :OccupationSection .
:JobTitleQuestion :inSection :OccupationSection .
:EmployerNameQuestion :inSection :OccupationSection .
:IndustryQuestion :inSection :OccupationSection .
:EmploymentStatusQuestion :inSection :OccupationSection .
:SecondOccupationQuestion :inSection :OccupationSection .
:SecondJobTitleQuestion :inSection :OccupationSection .

# ============================================================================
# Additional Properties for Section Management
# ============================================================================

:sectionId a owl:DatatypeProperty ;
    rdfs:label "section ID" ;
    rdfs:domain :Section ;
    rdfs:range xsd:string ;
    rdfs:comment "Unique identifier for the section" .

:sectionOrder a owl:DatatypeProperty ;
    rdfs:label "section order" ;
    rdfs:domain :Section ;
    rdfs:range xsd:integer ;
    rdfs:comment "The display order of sections in the dialog flow" .

# Inverse property for easier querying
:hasSection a owl:ObjectProperty ;
    rdfs:label "has section" ;
    rdfs:domain :Dialog ;
    rdfs:range :Section ;
    owl:inverseOf :inSection ;
    rdfs:comment "The sections contained in this dialog" .

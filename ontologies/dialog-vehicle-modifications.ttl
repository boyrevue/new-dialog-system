    skos:prefLabel "LPG Conversion"@en ;
    skos:altLabel "Gas Conversion"@en ;
    mod:modificationId "mod_lpg_conversion" ;
    mod:insuranceImpact "high" ;
    mod:requiresDeclaration true ;
    mod:affectsSafety true .

mod:ElectricConversion a mod:Modification ;
    rdfs:label "Electric / Hybrid Conversion" ;
    skos:prefLabel "Electric Conversion"@en ;
    skos:altLabel "EV Conversion"@en ;
    mod:modificationId "mod_electric_conversion" ;
    mod:insuranceImpact "very-high" ;
    mod:requiresDeclaration true ;
    mod:affectsSafety true .

mod:Conversion mod:hasModification mod:LPGConversion, mod:ElectricConversion .

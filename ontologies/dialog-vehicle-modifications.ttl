@prefix : <http://diggi.io/ontology/dialog#> .
@prefix mod: <http://diggi.io/ontology/modifications#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Vehicle Modifications Ontology
<http://diggi.io/ontology/modifications> a owl:Ontology ;
    rdfs:label "Vehicle Modifications Ontology" ;
    rdfs:comment "Hierarchical ontology of vehicle modifications for UK car insurance" ;
    owl:versionInfo "1.0" .

# Core Classes

mod:ModificationCategory a owl:Class ;
    rdfs:label "Modification Category" ;
    rdfs:comment "Top-level category of vehicle modification (e.g., Engine, Body, Suspension)" .

mod:ModificationSubcategory a owl:Class ;
    rdfs:subClassOf mod:ModificationCategory ;
    rdfs:label "Modification Subcategory" ;
    rdfs:comment "Second-level grouping of modifications" .

mod:Modification a owl:Class ;
    rdfs:label "Modification" ;
    rdfs:comment "Specific vehicle modification" .

# Properties

mod:modificationId a owl:DatatypeProperty ;
    rdfs:label "modification ID" ;
    rdfs:range xsd:string ;
    rdfs:comment "Unique identifier for the modification" .

mod:hasSubcategory a owl:ObjectProperty ;
    rdfs:label "has subcategory" ;
    rdfs:domain mod:ModificationCategory ;
    rdfs:range mod:ModificationSubcategory ;
    rdfs:comment "Links category to its subcategories" .

mod:hasModification a owl:ObjectProperty ;
    rdfs:label "has modification" ;
    rdfs:domain mod:ModificationSubcategory ;
    rdfs:range mod:Modification ;
    rdfs:comment "Links subcategory to specific modifications" .

mod:insuranceImpact a owl:DatatypeProperty ;
    rdfs:label "insurance impact" ;
    rdfs:range xsd:string ;
    rdfs:comment "Impact on insurance: 'low', 'medium', 'high', 'very-high'" .

mod:requiresDeclaration a owl:DatatypeProperty ;
    rdfs:label "requires declaration" ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Whether this modification must be declared to insurers" .

mod:affectsSafety a owl:DatatypeProperty ;
    rdfs:label "affects safety" ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Whether modification affects vehicle safety" .


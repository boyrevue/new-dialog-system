mm:minLength a owl:DatatypeProperty ;
    rdfs:label "minimum length" ;
    rdfs:domain mm:MultimodalQuestion ;
    rdfs:range xsd:integer ;
    rdfs:comment "Minimum length for text inputs" .

mm:maxLength a owl:DatatypeProperty ;
    rdfs:label "maximum length" ;
    rdfs:domain mm:MultimodalQuestion ;
    rdfs:range xsd:integer ;
    rdfs:comment "Maximum length for text inputs" .

mm:minValue a owl:DatatypeProperty ;
    rdfs:label "minimum value" ;
    rdfs:domain mm:MultimodalQuestion ;
    rdfs:range xsd:decimal ;
    rdfs:comment "Minimum value for number inputs" .

mm:maxValue a owl:DatatypeProperty ;
    rdfs:label "maximum value" ;
    rdfs:domain mm:MultimodalQuestion ;
    rdfs:range xsd:decimal ;
    rdfs:comment "Maximum value for number inputs" .

mm:pattern a owl:DatatypeProperty ;
    rdfs:label "pattern" ;
    rdfs:domain mm:MultimodalQuestion ;
    rdfs:range xsd:string ;
    rdfs:comment "Regular expression pattern for validation" .

mm:uiHint a owl:DatatypeProperty ;
    rdfs:label "UI hint" ;
    rdfs:domain mm:MultimodalQuestion ;

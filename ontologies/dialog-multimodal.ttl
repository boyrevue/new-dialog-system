mm:helpText a owl:DatatypeProperty ;
    rdfs:label "help text" ;
    rdfs:domain mm:MultimodalQuestion ;
    rdfs:range xsd:string ;
    rdfs:comment "Help text displayed below the field" .

mm:defaultValue a owl:DatatypeProperty ;
    rdfs:label "default value" ;
    rdfs:domain mm:MultimodalQuestion ;
    rdfs:range xsd:string ;
    rdfs:comment "Default value for the field" .

mm:validationRule a owl:DatatypeProperty ;
    rdfs:label "validation rule" ;
    rdfs:domain mm:MultimodalQuestion ;
    rdfs:range xsd:string ;

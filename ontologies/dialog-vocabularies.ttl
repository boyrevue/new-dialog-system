@prefix : <http://diggi.io/ontology/dialog#> .
@prefix vocab: <http://diggi.io/ontology/vocabularies#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Vocabulary Ontology
<http://diggi.io/ontology/vocabularies> a owl:Ontology ;
    rdfs:label "Dialog Vocabularies" ;
    rdfs:comment "SKOS controlled vocabularies for dialog system" ;
    owl:versionInfo "1.0" .

# Concept Schemes
vocab:VehicleTypes a skos:ConceptScheme ;
    skos:prefLabel "Vehicle Types"@en ;
    skos:definition "Classification of vehicle types for insurance purposes"@en .

vocab:CoverTypes a skos:ConceptScheme ;
    skos:prefLabel "Insurance Cover Types"@en ;
    skos:definition "Types of insurance coverage available"@en .

vocab:YesNoValues a skos:ConceptScheme ;
    skos:prefLabel "Yes/No Values"@en ;
    skos:definition "Boolean values with multiple acceptable expressions"@en .

vocab:UKRegions a skos:ConceptScheme ;
    skos:prefLabel "UK Regions"@en ;
    skos:definition "Regions of the United Kingdom"@en .

# Vehicle Type Concepts
vocab:Car a skos:Concept ;
    skos:prefLabel "Car"@en ;
    skos:altLabel "Motor Car"@en, "Automobile"@en, "Private Car"@en ;
    skos:definition "Private passenger motor vehicle"@en ;
    skos:inScheme vocab:VehicleTypes ;
    skos:notation "CAR" .

vocab:Van a skos:Concept ;
    skos:prefLabel "Van"@en ;
    skos:altLabel "Light Commercial Vehicle"@en, "LCV"@en ;
    skos:definition "Light commercial vehicle or van"@en ;
    skos:inScheme vocab:VehicleTypes ;
    skos:notation "VAN" .

vocab:Motorbike a skos:Concept ;
    skos:prefLabel "Motorbike"@en ;
    skos:altLabel "Motorcycle"@en, "Moped"@en, "Bike"@en ;
    skos:definition "Two-wheeled motor vehicle"@en ;
    skos:inScheme vocab:VehicleTypes ;
    skos:notation "BIKE" .

vocab:Taxi a skos:Concept ;
    skos:prefLabel "Taxi"@en ;
    skos:altLabel "Hackney Carriage"@en, "Private Hire"@en, "Cab"@en ;
    skos:definition "Licensed passenger transport vehicle"@en ;
    skos:inScheme vocab:VehicleTypes ;
    skos:notation "TAXI" .

# Cover Type Concepts
vocab:ThirdParty a skos:Concept ;
    skos:prefLabel "Third Party"@en ;
    skos:altLabel "TP"@en, "Third Party Only"@en, "TPO"@en ;
    skos:definition "Basic legal minimum coverage"@en ;
    skos:inScheme vocab:CoverTypes ;
    skos:notation "TP" .

vocab:ThirdPartyFireTheft a skos:Concept ;
    skos:prefLabel "Third Party, Fire and Theft"@en ;
    skos:altLabel "TPFT"@en, "TP Fire & Theft"@en ;
    skos:definition "Third party cover plus fire and theft protection"@en ;
    skos:inScheme vocab:CoverTypes ;
    skos:notation "TPFT" .

vocab:Comprehensive a skos:Concept ;
    skos:prefLabel "Comprehensive"@en ;
    skos:altLabel "Fully Comprehensive"@en, "Comp"@en, "Full Cover"@en ;
    skos:definition "Complete coverage including own vehicle damage"@en ;
    skos:inScheme vocab:CoverTypes ;
    skos:notation "COMP" .

# Yes/No Value Concepts
vocab:Yes a skos:Concept ;
    skos:prefLabel "Yes"@en ;
    skos:altLabel "Y"@en, "Yeah"@en, "Yep"@en, "Yup"@en, "Correct"@en, "True"@en, "Affirmative"@en ;
    skos:definition "Positive or affirmative response"@en ;
    skos:inScheme vocab:YesNoValues ;
    skos:notation "YES" .

vocab:No a skos:Concept ;
    skos:prefLabel "No"@en ;
    skos:altLabel "N"@en, "Nope"@en, "Nah"@en, "Negative"@en, "False"@en, "Incorrect"@en ;
    skos:definition "Negative or denial response"@en ;
    skos:inScheme vocab:YesNoValues ;
    skos:notation "NO" .


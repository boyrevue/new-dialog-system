    vocab:usesSpaceSeparator true ;
    rdfs:comment "Input configuration for UK postcodes - supports letter-by-letter spelling" .

vocab:LicenceInputMode a vocab:InputMode ;
    rdfs:label "Driving Licence Input Mode" ;

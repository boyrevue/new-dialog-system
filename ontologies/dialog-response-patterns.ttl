@prefix : <http://diggi.io/ontology/dialog#> .
@prefix resp: <http://diggi.io/ontology/response#> .
@prefix asr: <http://diggi.io/ontology/asr#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# ASR Response Patterns Ontology
<http://diggi.io/ontology/response-patterns> a owl:Ontology ;
    rdfs:label "ASR Response Patterns Ontology" ;
    rdfs:comment "Comprehensive collection of ASR response patterns for natural language understanding" ;
    owl:versionInfo "1.0" .

# Core Classes

resp:ResponsePatternCategory a owl:Class ;
    rdfs:label "Response Pattern Category" ;
    rdfs:comment "Category grouping for response patterns (e.g., direct, possessive, conversational)" .

resp:ResponsePattern a owl:Class ;
    rdfs:label "Response Pattern" ;
    rdfs:comment "A natural language pattern that users might use to respond to questions" .

resp:PatternType a owl:Class ;
    rdfs:label "Pattern Type" ;
    rdfs:comment "Type of response pattern (simple, compound, formal, informal)" .

# Properties

resp:hasPattern a owl:DatatypeProperty ;
    rdfs:label "has pattern" ;
    rdfs:domain resp:ResponsePattern ;
    rdfs:range xsd:string ;
    rdfs:comment "The pattern template with <answer> placeholder" .

resp:hasCategory a owl:ObjectProperty ;
    rdfs:label "has category" ;
    rdfs:domain resp:ResponsePattern ;
    rdfs:range resp:ResponsePatternCategory ;
    rdfs:comment "Links pattern to its category" .

resp:hasExampleValue a owl:DatatypeProperty ;
    rdfs:label "has example value" ;
    rdfs:domain resp:ResponsePattern ;
    rdfs:range xsd:string ;
    rdfs:comment "Example utterance using this pattern" .

resp:confidenceWeight a owl:DatatypeProperty ;
    rdfs:label "confidence weight" ;
    rdfs:domain resp:ResponsePattern ;
    rdfs:range xsd:float ;
    rdfs:comment "Weight for confidence scoring (0.0-1.0)" .

resp:formality a owl:DatatypeProperty ;
    rdfs:label "formality" ;
    rdfs:domain resp:ResponsePattern ;
    rdfs:range xsd:string ;
    rdfs:comment "Formality level: formal, neutral, informal, casual" .

resp:appliesToFieldType a owl:DatatypeProperty ;
    rdfs:label "applies to field type" ;
    rdfs:domain resp:ResponsePattern ;
    rdfs:range xsd:string ;
    rdfs:comment "Field types this pattern applies to (name, email, phone, etc.)" .

# Categories

resp:DirectAnswerCategory a resp:ResponsePatternCategory ;
    rdfs:label "Direct Answer" ;
    skos:definition "Simple direct answer without preamble" .

resp:PossessiveCategory a resp:ResponsePatternCategory ;
    rdfs:label "Possessive" ;
    skos:definition "Answers using possessive pronouns (my X is, the X is)" .

resp:DemonstrativeCategory a resp:ResponsePatternCategory ;
    rdfs:label "Demonstrative" ;
    skos:definition "Answers using demonstrative pronouns (it's, that's, this is)" .

resp:ConversationalCategory a resp:ResponsePatternCategory ;
    rdfs:label "Conversational" ;
    skos:definition "Conversational responses with affirmations (sure, okay, yes)" .

resp:FormalCategory a resp:ResponsePatternCategory ;
    rdfs:label "Formal" ;
    skos:definition "Polite and formal responses (certainly, of course)" .

resp:HedgingCategory a resp:ResponsePatternCategory ;
    rdfs:label "Hedging" ;
    skos:definition "Uncertain or hedging responses (I think, probably)" .

resp:TemporalCategory a resp:ResponsePatternCategory ;
    rdfs:label "Temporal" ;
    skos:definition "Time-qualified responses (right now, currently)" .

resp:NameSpecificCategory a resp:ResponsePatternCategory ;
    rdfs:label "Name-Specific" ;
    skos:definition "Patterns specific to name questions" .

# ========================================
# DIRECT ANSWER PATTERNS
# ========================================

resp:DirectAnswer a resp:ResponsePattern ;
    resp:hasPattern "<answer>" ;
    resp:hasCategory resp:DirectAnswerCategory ;
    resp:hasExampleValue "John" ;
    resp:confidenceWeight 1.0 ;
    resp:formality "neutral" ;
    resp:appliesToFieldType "name, email, phone, postcode, address, generic" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dvla: <http://dvla.example.org/ontology#> .

# ============================================================================
# UK DVLA DRIVING OFFENCES ONTOLOGY
# Hierarchical structure: OffenceCategory → OffenceCode → Penalty Details
# ============================================================================

<http://dvla.example.org/ontology>
    a owl:Ontology ;
    rdfs:label "UK DVLA Driving Offences Ontology"@en ;
    rdfs:comment "Comprehensive ontology for UK driving offence codes and penalties"@en ;
    owl:versionInfo "1.0" .

# ============================================================================
# CLASSES
# ============================================================================

dvla:OffenceCategory a owl:Class ;
    rdfs:label "Offence Category"@en ;
    rdfs:comment "Top-level category of driving offences (AC, BA, CD, etc.)"@en .

dvla:OffenceCode a owl:Class ;
    rdfs:label "Offence Code"@en ;
    rdfs:comment "Specific offence code (e.g., SP30, DR10)"@en .

dvla:Penalty a owl:Class ;
    rdfs:label "Penalty"@en ;
    rdfs:comment "Penalty details for an offence"@en .

# ============================================================================
# PROPERTIES
# ============================================================================

dvla:hasOffenceCode a owl:ObjectProperty ;
    rdfs:domain dvla:OffenceCategory ;
    rdfs:range dvla:OffenceCode ;
    rdfs:label "has offence code"@en .

dvla:hasPenalty a owl:ObjectProperty ;
    rdfs:domain dvla:OffenceCode ;
    rdfs:range dvla:Penalty ;
    rdfs:label "has penalty"@en .

dvla:code a owl:DatatypeProperty ;
    rdfs:domain dvla:OffenceCode ;
    rdfs:range xsd:string ;
    rdfs:label "offence code"@en .

dvla:description a owl:DatatypeProperty ;
    rdfs:domain dvla:OffenceCode ;
    rdfs:range xsd:string ;
    rdfs:label "offence description"@en .

dvla:penaltyPoints a owl:DatatypeProperty ;
    rdfs:domain dvla:Penalty ;
    rdfs:range xsd:string ;
    rdfs:label "penalty points"@en ;
    rdfs:comment "Range of penalty points (e.g., '3-6')"@en .

dvla:disqualificationPeriod a owl:DatatypeProperty ;
    rdfs:domain dvla:Penalty ;
    rdfs:range xsd:string ;
    rdfs:label "disqualification period"@en .

dvla:mustDisqualify a owl:DatatypeProperty ;
    rdfs:domain dvla:Penalty ;
    rdfs:range xsd:boolean ;
    rdfs:label "must disqualify"@en .


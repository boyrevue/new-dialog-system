@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ins: <http://insurance.example.org/ontology#> .
@prefix veh: <http://vehicle.example.org/ontology#> .

# ============================================================================
# VEHICLE INSURANCE ONTOLOGY
# Hierarchical structure: Section → Question → Field → Vehicle Specs
# ============================================================================

<http://insurance.example.org/ontology>
    a owl:Ontology ;
    rdfs:label "Vehicle Insurance Ontology"@en ;
    rdfs:comment "Hierarchical ontology for vehicle insurance data collection"@en ;
    owl:versionInfo "1.0" .

# ============================================================================
# CLASSES
# ============================================================================

ins:Section a owl:Class ;
    rdfs:label "Insurance Section"@en ;
    rdfs:comment "Top-level grouping for related questions"@en .

ins:Question a owl:Class ;
    rdfs:label "Insurance Question"@en ;
    rdfs:comment "Question asked to user"@en .

ins:Field a owl:Class ;
    rdfs:label "Data Field"@en ;
    rdfs:comment "Field to collect data"@en .

veh:Manufacturer a owl:Class ;
    rdfs:label "Vehicle Manufacturer"@en ;
    rdfs:comment "Car manufacturer (Level 1)"@en .

veh:Model a owl:Class ;
    rdfs:label "Vehicle Model"@en ;
    rdfs:comment "Car model (Level 2)"@en .

veh:VehicleSpec a owl:Class ;
    rdfs:label "Vehicle Specification"@en ;
    rdfs:comment "Technical specifications of a vehicle"@en .

# ============================================================================
# PROPERTIES
# ============================================================================

ins:hasQuestion a owl:ObjectProperty ;
    rdfs:domain ins:Section ;
    rdfs:range ins:Question ;
    rdfs:label "has question"@en .

ins:hasField a owl:ObjectProperty ;
    rdfs:domain ins:Question ;
    rdfs:range ins:Field ;
    rdfs:label "has field"@en .

veh:hasModel a owl:ObjectProperty ;
    rdfs:domain veh:Manufacturer ;
    rdfs:range veh:Model ;
    rdfs:label "has model"@en .

veh:hasSpec a owl:ObjectProperty ;
    rdfs:domain veh:Model ;
    rdfs:range veh:VehicleSpec ;
    rdfs:label "has specification"@en .

# Specification Properties
veh:engineSize a owl:DatatypeProperty ;
    rdfs:domain veh:VehicleSpec ;
    rdfs:range xsd:decimal ;
    rdfs:label "engine size"@en ;
    rdfs:comment "Engine size in litres"@en .

veh:fuelType a owl:DatatypeProperty ;
    rdfs:domain veh:VehicleSpec ;
    rdfs:range xsd:string ;
    rdfs:label "fuel type"@en .

veh:numberOfDoors a owl:DatatypeProperty ;
    rdfs:domain veh:VehicleSpec ;
    rdfs:range xsd:integer ;
    rdfs:label "number of doors"@en .

veh:numberOfSeats a owl:DatatypeProperty ;
    rdfs:domain veh:VehicleSpec ;
    rdfs:range xsd:integer ;
    rdfs:label "number of seats"@en .

veh:transmission a owl:DatatypeProperty ;
    rdfs:domain veh:VehicleSpec ;
    rdfs:range xsd:string ;
    rdfs:label "transmission"@en .

veh:bodyType a owl:DatatypeProperty ;
    rdfs:domain veh:VehicleSpec ;
    rdfs:range xsd:string ;
    rdfs:label "body type"@en .

veh:insuranceGroup a owl:DatatypeProperty ;
    rdfs:domain veh:VehicleSpec ;
    rdfs:range xsd:integer ;
    rdfs:label "insurance group"@en ;
    rdfs:comment "UK insurance group (1-50)"@en .

# ============================================================================
# VEHICLE MANUFACTURERS (Level 1)
# ============================================================================

veh:Toyota a veh:Manufacturer ;
    rdfs:label "Toyota"@en ;
    skos:prefLabel "Toyota"@en .

veh:BMW a veh:Manufacturer ;
    rdfs:label "BMW"@en ;
    skos:prefLabel "BMW"@en .

veh:Mercedes a veh:Manufacturer ;
    rdfs:label "Mercedes-Benz"@en ;
    skos:prefLabel "Mercedes"@en ;
    skos:altLabel "Merc"@en .

veh:Ford a veh:Manufacturer ;
    rdfs:label "Ford"@en ;
    skos:prefLabel "Ford"@en .

veh:Volkswagen a veh:Manufacturer ;
    rdfs:label "Volkswagen"@en ;
    skos:prefLabel "Volkswagen"@en ;
    skos:altLabel "VW"@en .

veh:Audi a veh:Manufacturer ;
    rdfs:label "Audi"@en ;
    skos:prefLabel "Audi"@en .

veh:Honda a veh:Manufacturer ;
    rdfs:label "Honda"@en ;
    skos:prefLabel "Honda"@en .

veh:Nissan a veh:Manufacturer ;
    rdfs:label "Nissan"@en ;
    skos:prefLabel "Nissan"@en .

# ============================================================================
# VEHICLE MODELS (Level 2) with Specifications
# ============================================================================

# Toyota Models
veh:ToyotaCorolla a veh:Model ;
    rdfs:label "Corolla"@en ;
    skos:prefLabel "Corolla"@en ;
    veh:manufacturer veh:Toyota ;
    veh:hasSpec veh:ToyotaCorollaSpec .

veh:ToyotaCorollaSpec a veh:VehicleSpec ;
    veh:engineSize 1.8 ;
    veh:fuelType "Petrol" ;
    veh:numberOfDoors 4 ;
    veh:numberOfSeats 5 ;
    veh:transmission "Manual" ;
    veh:bodyType "Saloon" ;
    veh:insuranceGroup 15 .

veh:ToyotaPrius a veh:Model ;
    rdfs:label "Prius"@en ;

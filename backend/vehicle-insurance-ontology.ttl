# ============================================================================
# FUEL TYPES (SKOS Controlled Vocabulary)
# ============================================================================

veh:FuelTypeScheme a skos:ConceptScheme ;
    rdfs:label "Fuel Types"@en .

veh:Petrol a skos:Concept ;
    skos:prefLabel "Petrol"@en ;
    skos:inScheme veh:FuelTypeScheme .

veh:Diesel a skos:Concept ;
    skos:prefLabel "Diesel"@en ;
    skos:inScheme veh:FuelTypeScheme .

veh:Electric a skos:Concept ;
    skos:prefLabel "Electric"@en ;
    skos:altLabel "EV"@en ;
    skos:inScheme veh:FuelTypeScheme .

veh:Hybrid a skos:Concept ;
    skos:prefLabel "Hybrid"@en ;
    skos:altLabel "Petrol-Electric"@en ;
    skos:inScheme veh:FuelTypeScheme .

veh:PlugInHybrid a skos:Concept ;
    skos:prefLabel "Plug-in Hybrid"@en ;
    skos:altLabel "PHEV"@en ;
    skos:inScheme veh:FuelTypeScheme .

# ============================================================================
# BODY TYPES (SKOS Controlled Vocabulary)

    veh:transmission "Automatic" ;
    veh:bodyType "SUV" ;
    veh:insuranceGroup 42 .

# Mercedes Models
veh:MercedesCClass a veh:Model ;
    rdfs:label "C-Class"@en ;
    skos:prefLabel "C-Class"@en ;
